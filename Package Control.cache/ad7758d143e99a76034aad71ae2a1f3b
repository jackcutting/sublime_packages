ard/sublime-jsrevival/master/packages.json": [{"issues": null, "readme": null, "donate": null, "name": "jsrevival", "previous_names": [], "description": "Another interface to jslint. Base on jsrevival node package. Require node and jsrevival", "homepage": "https://github.com/sdolard/sublime-jsrevival", "author": "sdolard <sdolard@gmail.com>", "releases": [{"version": "0.1.1", "sublime_text": "<3000", "date": "2014-01-18 23:08:00", "platforms": ["*"], "url": "https://codeload.github.com/sdolard/sublime-jsrevival/zip/v0.1.1"}], "buy": null, "labels": []}], "https://raw.github.com/sentience/HyperlinkHelper/master/packages.json": [{"issues": "https://github.com/sentience/HyperlinkHelper/issues", "readme": "https://raw.github.com/sentience/HyperlinkHelper/master/README.md", "donate": "https://www.gittip.com/on/github/sentience/", "name": "HyperlinkHelper", "previous_names": [], "description": "Quickly create hyperlinks from selected text or clipboard contents", "homepage": "https://github.com/sentience/HyperlinkHelper", "author": "Kevin Yank", "releases": [{"version": "1.4.2+st3", "sublime_text": ">=3000", "date": "2014-03-13 09:00:00", "platforms": ["*"], "url": "https://github.com/sentience/HyperlinkHelper/archive/1.4.2+st3.zip"}, {"version": "1.4.2", "sublime_text": "<3000", "date": "2014-03-13 09:00:00", "platforms": ["*"], "url": "https://github.com/sentience/HyperlinkHelper/archive/1.4.2.zip"}], "buy": null, "labels": ["formatting", "text manipulation"]}], "https://raw.github.com/Mozillion/SublimeSpeech/master/packages.json": [{"issues": null, "readme": null, "donate": null, "name": "Speech", "previous_names": [], "description": "Speech recognition for Sublime Speech 2", "homepage": "https://github.com/Mozillion/SublimeSpeech", "author": "Mozillion", "releases": [{"version": "1.0.1", "sublime_text": "<3000", "date": "2013-01-27 00:00:00", "platforms": ["windows"], "url": "https://codeload.github.com/Mozillion/SublimeSpeech/zip/v1.0.1"}], "buy": null, "labels": []}]